<?xml version="1.0"?>
<launch>

  <include file="$(find wheeltec_basics)/launch/robot_model.launch" />

  <include file="$(find wheeltec_basics)/launch/gazebo.launch" />

  <!-- Spawn a robot into Gazebo -->
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" 
    args="-param robot_description -urdf -model wheeltec" />

</launch>
